{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=test SEQUELIZE_DB_CONFIG=db/config/database.json mocha --require babel-register --exit",
    "create-db-test": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:create finance-home-test"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "cross-env": "^5.1.4",
    "dotenv": "^5.0.1",
    "express": "^4.13.3",
    "express-jwt": "^5.3.1",
    "morgan": "^1.8.0",
    "mysql2": "^1.5.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "^4.37.1",
    "sequelize-cli": "^4.0.0",
    "winston": "^2.4.1",
    "jsonwebtoken": "^8.2.0",
    "db-migrate": "^0.10.5",
    "db-migrate-sqlite3": "^0.2.1"
  },
  "devDependencies": {
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^3.1.1",

    "mocha": "^5.0.4",
    "nodemon": "^1.9.2"
  },
  "babel": {
    "presets": [
      "stage-3",
      "latest"
    ]
  }
}
